{
  "name": "testing-library-table-queries",
  "version": "0.1.2",
  "description": "Additional testing-library queries for querying tables like a user",
  "author": "lexanth",
  "license": "MIT",
  "repository": "lexanth/testing-library-table-queries",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "microbundle --format cjs --target node",
    "start": "microbundle watch --format cjs --target node",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.5.1",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@testing-library/dom": "^7.5.1"
  },
  "files": [
    "dist"
  ]
}
